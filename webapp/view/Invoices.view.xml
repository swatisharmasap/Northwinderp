<mvc:View
    controllerName="sap.com.northwinderp.controller.Invoices"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:m="sap.m"
    xmlns="sap.ui.table"
>
<m:Page
        showHeader="false"
        enableScrolling="false"
        class="sapUiContentPadding"
    >
    <m:content>
    <Table
                rows="{invoicesModel>/}"
                selectionMode="MultiToggle"
                paste="onPaste"
                ariaLabelledBy="title"
                selectionBehavior="RowOnly"
                rowSelectionChange="onRowPress"
            >
           
            <columns>
            <Column width="11rem">
                        <m:Label text="ShipName"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>ShipName}"
                                wrapping="false"
                            />
                        </template>
                    </Column>
                    <Column width="11rem">
                        <m:Label text="ShipAddress"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>ShipAddress}"
                                wrapping="false"
                            />
                        </template>
                    </Column>
                    <Column width="11rem">
                        <m:Label text="ShipCity"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>ShipCity}"
                                wrapping="false"
                            />
                        </template>
                    </Column>
                    <Column width="11rem">
                        <m:Label text="ShipRegion"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>ShipRegion}"
                                wrapping="false"
                            />
                        </template>
                    </Column> 
                    <Column width="11rem">
                        <m:Label text="ShipPostalCode"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>ShipPostalCode}"
                                wrapping="false"
                            />
                        </template>
                    </Column> 
                    <Column width="11rem">
                        <m:Label text="ShipCountry"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>ShipCountry}"
                                wrapping="false"
                            />
                        </template>
                    </Column> 
                    <Column width="11rem">
                        <m:Label text="CustomerID"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>CustomerID}"
                                wrapping="false"
                            />
                        </template>
                    </Column> 
                    <Column width="11rem">
                        <m:Label text="CustomerName"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>CustomerName}"
                                wrapping="false"
                            />
                        </template>
                    </Column>
                     <Column width="11rem">
                        <m:Label text="Address"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>Address}"
                                wrapping="false"
                            />
                        </template>
                    </Column>
                    <Column width="11rem">
