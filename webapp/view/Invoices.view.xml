                        <template>
                            <m:Text
                                text="{invoicesModel>ProductID}"
                                wrapping="false"
                            />
                        </template>
                    </Column>
                    <Column width="11rem">
                        <m:Label text="ProductName"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>ProductName}"
                                wrapping="false"
                            />
                        </template>
                    </Column>
                    <Column width="11rem">
                        <m:Label text="UnitPrice"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>UnitPrice}"
                                wrapping="false"
