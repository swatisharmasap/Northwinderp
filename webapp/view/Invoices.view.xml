                        <template>
                            <m:Text
                                text="{invoicesModel>OrderID}"
                                wrapping="false"
                            />
                        </template>
                    </Column>
                    <Column width="11rem">
                        <m:Label text="OrderDate"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>OrderDate}"
                                wrapping="false"
                            />
                        </template>
                    </Column>
                    <Column width="11rem">
