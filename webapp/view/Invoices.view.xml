                                text="{invoicesModel>Discount}"
                                wrapping="false"
                            />
                        </template>
                    </Column>
                    <Column width="11rem">
                        <m:Label text="ExtendedPrice"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>ExtendedPrice}"
                                wrapping="false"
                            />
                        </template>
                    </Column>
                    <Column width="11rem">
                        <m:Label text="Freight"/>
                        <template>
                            <m:Text
                                text="{invoicesModel>Freight}"
